		</etl:record>
	</etl:dataSet>

	<etl:dataSet id="fromFile" schema-ref="inputSchema">
		<etl:fileResource location="classpath:data/companylist.csv">
			<etl:selectLines value="2-4" />
		</etl:fileResource>
	</etl:dataSet>

	<etl:test id="copyTest" name="Copy Test">
		<etl:filter ref="copyFilter" />
		<etl:when>
			<etl:input ref="mapperOutput" />
			<etl:dataSet ref="fromFile" />
		</etl:when>
		<etl:expect>
			<etl:output ref="copyOutputToSort" />
			<etl:dataSet ref="fromFile" />
		</etl:expect>
	</etl:test>

</beans>