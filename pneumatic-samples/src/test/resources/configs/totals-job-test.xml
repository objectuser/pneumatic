			<etl:columnValue column="Name" value="1347 Capital Corp." />
			<etl:columnValue column="LastSale" value="9.56" />
			<etl:columnValue column="MarketCap" value="56862880" />
			<etl:columnValue column="ADR TSO" value="n/a" />
